{
    "name": "josch1710/dojo-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for Dojo Toolkit JavaScript library (toolkit)",
    "keywords": ["symfony", "dojo", "dojo toolkit", "javascript"],
    "homepage": "http://github.com/josch1710/MmDojoBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Kokot",
            "email": "peter.kokot@m-m.si"
        },
        {
            "name": "Jochen Sch√§fer",
            "email": "josch1710@live.de"
        }
    ],
    "repositories": {
        "type": "package",
        "target-dir": "Resources/public/js",
        "package": {
            "name": "dojo/dojo",
            "version": "1.9.1",
            "dist": {
                "url": "http://download.dojotoolkit.org/release-1.9.1/dojo-release-1.9.1.zip",
                "type": "zip"
            },
            "source": {
                "url": "http://download.dojotoolkit.org/release-1.9.1/dojo-release-1.9.1-src.zip",
                "type": "zip"
            }
        }
    },
    "autoload": {
        "psr-0": { "Dojo\\DojoBundle": "" }
    },
    "require": {
        "symfony/framework-bundle": "2.1.*",
        "dojo/dojo": "1.9.1"
    },
    "target-dir": "Dojo/DojoBundle"
}